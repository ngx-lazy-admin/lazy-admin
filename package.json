{
  "name": "go-admin-ng",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve": "ng serve",
    "s": "ng serve",
    "build": "ng build --stats-json",
    "watch": "ng build --watch --configuration development",
    "docs": "ng build --output-path docs --base-href / & cp docs/index.html docs/404.html & echo lazy-admin-pro.com > docs/CNAME",
    "website": "ng build --output-path docs --base-href / & cp docs/index.html docs/404.html & echo lazy-admin-pro.com > docs/CNAME",
    "openApi": "java -jar ./swagger/swagger-codegen-cli.jar generate -i ./swagger/swagger.json -l typescript-angular -o ./src/app/services/api/swagger",
    "bundle": "webpack-bundle-analyzer dist/go-admin-ng/stats.json",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer ./dist/greeting-app/stats-es2015.json",
    "source": "ng build --source-map",
    "bundle:report": "source-map-explorer dist/go-admin-ng/**/*.js",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^14.0.1",
