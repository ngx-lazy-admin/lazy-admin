    "@antv/g6": "^4.5.1",
    "@antv/s2": "^1.3.0",
    "@ngx-formly/core": "^5.12",
    "bootstrap": "^5.1.0",
    "echarts": "^5.2.1",
    "graphql": "^16.4.0",
    "highcharts": "^9.3.1",
    "hotkeys-js": "^3.8.7",
    "marked": "^4.0.14",
    "mockjs": "^1.1.0",
    "monaco-editor": "^0.28.1",
